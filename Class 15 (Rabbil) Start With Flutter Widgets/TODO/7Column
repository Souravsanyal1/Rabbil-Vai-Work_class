class HomePage extends StatelessWidget{

  @override
  Widget build(BuildContext context) {
    return Scaffold(
      appBar: AppBar(title: Text("Flutter B4",style: TextStyle(color: Colors.blue),),),
      body: Column(
        children: [
          Container(width: 50, height: 100, color: Colors.amber),
          SizedBox(height: 20),
          Container(width: 50, height: 100, color: Colors.red),
          SizedBox(height: 20),
          Container(width: 50, height: 100, color: Colors.blue),
          SizedBox(height: 20),
          Container(width: 50, height: 100, color: Colors.green),
        ],
      )
    );
  }
}


class HomePage extends StatelessWidget{
  @override
  Widget build(BuildContext context) {
    return Scaffold(
      appBar: AppBar(title: Text("Flutter B4",style: TextStyle(color: Colors.blue),),),
      body: Column(
        mainAxisAlignment: MainAxisAlignment.center,
        children: [
          Container(width: 50, height: 100, color: Colors.amber),

          SizedBox(width: 20,height: 20),
          Container(width: 50, height: 100, color: Colors.red),
          SizedBox(width: 20,height: 20),
          Container(width: 50, height: 100, color: Colors.blue),
          SizedBox(width: 20,height: 20),
          Container(width: 50, height: 100, color: Colors.green),
        ],
      )
    );
  }
}